{
  "name": "linefusion",
  "version": "0.0.1",
  "description": "Automated lidir power line extraction and fitting",
  "main": "main.js",
  "devDependencies": {
    "browserify": "^17.0.0",
    "browserify-shim": "^3.8.16",
    "chai": "^4.3.7",
    "classnames": "^2.3.2",
    "connect": "^3.7.0",
    "create-react-class": "^15.7.0",
    "electron": "^34.3.0",
    "electron-builder": "^25.1.8",
    "envify": "^4.1.0",
    "greyhound.js": "0.0.8",
    "gulp": "^4.0.2",
    "gulp-awspublish": "^3.3.0",
    "gulp-clean": "^0.4.0",
    "gulp-closure-compiler": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-html-replace": "^1.6.2",
    "gulp-jshint": "^2.1.0",
    "gulp-less": "^5.0.0",
    "gulp-livereload": "^4.0.2",
    "gulp-mocha": "^8.0.0",
    "gulp-nodemon": "^2.5.0",
    "gulp-react": "^3.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.1",
    "icon-gen": "^5.0.0",
    "jshint": "^2.13.6",
    "jshint-jsx": "^0.5.0",
    "mochify": "^9.0.0",
    "morgan": "^1.10.0",
    "mori": "^0.3.2",
    "open": "^8.4.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "reactify": "^1.1.1",
    "serve-static": "^1.15.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:verma/plasio.git"
  },
  "author": "CCH&ZHY&WBS",
  "license": "MIT",
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./vendor/jquery.js",
    "three": "./vendor/three.js",
    "bluebird": "./vendor/bluebird.js",
    "jqueryui": "./vendor/jquery.ui.js",
    "jquery-nouislider": "./vendor/jquery.nouislider.min.js",
    "jquery-layout": "./vendor/jquery.layout.js",
    "bootstrap": "./vendor/bootstrap.min.js",
    "trackball-controls": "./vendor/controls/TrackballControls.js",
    "binary-loader": "./vendor/loaders/BinaryLoader.js",
    "lodash": "./vendor/lodash.min.js"
  },
  "browserify-shim": {
    "bluebird": "global:Promise",
    "jquery": "$",
    "jqueryui": {
      "depends": "jquery:$"
    },
    "jquery-layout": {
      "depends": "jquery:$"
    },
    "jquery-nouislider": {
      "depends": "jquery:$"
    },
    "three": "THREE",
    "trackball-controls": {
      "depends": "three:THREE"
    },
    "binary-loader": {
      "depends": "three:THREE"
    },
    "bootstrap": {
      "depends": "jquery:$"
    }
  },
  "dependencies": {
    "electron-log": "^5.0.1",
    "iconv-lite": "^0.6.3",
    "puppeteer": "^24.3.0"
  },
  "build": {
    "appId": "com.zxrz.linefusion",
    "productName": "Linefusion",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "main.js",
      "package.json",
      "node_modules/**/*",
      "preload.js",
      "process_1.py",
      "process_2.py",
      "process_3.py"
    ],
    "extraResources": [
      {
        "from": "build/",
        "to": "build",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "resources/python",
        "to": "python"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "images/linefusion.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Linefusion",
      "installerIcon": "images/install.ico",
      "uninstallerIcon": "images/uninstall.ico"
    },
    "npmRebuild": false,
    "buildDependenciesFromSource": false,
    "asarUnpack": [
      "preload.js",
      "process_1.py",
      "process_2.py",
      "process_3.py",
      "resources/python/**/*"
    ],
    "asar": true
  }
}
